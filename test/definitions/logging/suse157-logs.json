{
    "global_tags": {
        "owning_team": "virtuoso",
        "Environment": "development",
        "Department": "product",
        "Product": "virtuoso"
    },
    "resources": [
        {
            "id": "suse157-logs",
            "provider": "aws",
            "type": "ec2",
            "size": "t3.micro",
            "ami_name": "suse-sles-15-sp7-v*-hvm-ssd-x86_64"
        }
    ],
    "instrumentations": {
        "resources": [
            {
                "id": "nr_infra_suse157",
                "resource_ids": [
                    "suse157-logs"
                ],
                "provider": "newrelic",
                "source_repository": "https://github.com/newrelic/open-install-library",
                "deploy_script_path": "test/deploy/linux/newrelic-cli/install/roles",
                "params": {
                    "newrelic_cli_option": "-n logs-integration",
                    "validate_output": "Logs Integration\\s+\\(installed\\)",
                    "local_recipes": true
                }
            }
        ]
    }
}
