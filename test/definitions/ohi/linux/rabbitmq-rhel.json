{
    "global_tags": {
      "owning_team": "OpenSource",
      "Environment": "development",
      "Department": "Product",
      "Product": "Virtuoso"
    },
    "resources": [
      {
        "id": "host1",
        "display_name": "AwsLinux2InfraRabbitMQInstallHost",
        "provider": "aws",
        "type": "ec2",
        "size": "t3.nano",
        "ami_name": "amazonlinux-2-base*"
      }
    ],
    "services": [
      {
        "id": "rabbitmq1",
        "destinations": [
          "host1"
        ],
        "source_repository": "https://github.com/newrelic/open-install-library.git",
        "deploy_script_path": "test/deploy/linux/rabbitmq/install/rhel/roles",
        "port": 80,
        "params": {
          "create_env_var": true,
          "create_newrelic_user": true
        }
      }
    ],
    "instrumentations": {
      "resources": [
        {
          "id": "nr_infra_rabbitmq",
          "resource_ids": [
            "host1"
          ],
          "provider": "newrelic",
          "source_repository": "https://github.com/newrelic/open-install-library.git",
          "deploy_script_path": "test/deploy/linux/newrelic-cli/install-recipe/roles",
          "params": {
            "recipe_content_url": "https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/infrastructure/ohi/rabbitmq/rhel.yml",
            "validate_output": "New Relic installation complete"
          }
        }
      ]
    }
  }
  