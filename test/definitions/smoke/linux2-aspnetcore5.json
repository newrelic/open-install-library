{
  "global_tags": {
    "owning_team": "OpenSource",
    "Environment": "development",
    "Department": "Product",
    "Product": "Virtuoso"
  },
  "resources": [{
      "id": "linux2host1",
      "display_name": "AwsLinux2ASPNETCore",
      "provider": "aws",
      "type": "ec2",
      "size": "t3.micro",
      "ami_name": "amazonlinux-2-base*",
      "user_name": "ec2-user"
    }],
  "services": [{
      "id": "aspnetcore",
      "destinations": [ "linux2host1" ],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/dotNet/install/rhel/roles",
      "port": 9999
    },
    {
      "id": "apache1",
      "destinations": [ "linux2host1" ],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/apache/install/rhel/roles",
      "port": 80
    },
    {
      "id": "dotnet1",
      "destinations": [ "linux2host1" ],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/apache/deploy-application/dotNet/rhel/roles",
      "port": 8080,
      "params": {
        "is_selfcontained": "true",
        "use_executable": "true",
        "proxy_port": "5001"
      }
    },
    {
      "id": "dotnet2",
      "destinations": [ "linux2host1" ],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/apache/deploy-application/dotNet/rhel/roles",
      "port": 8081,
      "params": {
        "proxy_port": "5000"
      }
    }]
    ,
  "instrumentations": {
    "resources": [
      {
        "id": "cli1",
        "resource_ids": ["linux2host1"],
        "provider": "newrelic",
        "source_repository": "https://github.com/newrelic/open-install-library.git",
        "deploy_script_path": "test/deploy/linux/newrelic-cli/install-recipe/roles",
        "params": {
          "recipe_content_url": "https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/infrastructure/amazonlinux_2.yml,https://raw.githubusercontent.com/newrelic/open-install-library/main/recipes/newrelic/apm/dotNet/linux-systemd.yml",
          "validate_output": "New Relic installation complete"
        }
      }
    ]
  }
}