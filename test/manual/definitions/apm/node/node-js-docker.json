{
    "global_tags": {
        "owning_team": "virtuoso",
        "Environment": "development",
        "Department": "product",
        "Product": "virtuoso"
    },
    "resources": [
        {
            "id": "node-js-docker",
            "provider": "aws",
            "type": "ec2",
            "size": "t3.micro",
            "volumes": [
                {
                    "size": 16,
                    "type": "gp3"
                }
            ],
            "ami_name": "al2023-ami-2023.4.20240416.0-kernel-6.1-x86_64",
            "user_name": "ec2-user"
        }
    ],
    "services": [
        {
            "id": "nodedocker",
            "local_source_path": "/mnt/deployer/",
            "deploy_script_path": "test/deploy/linux/nodejs/docker/redhat/roles",
            "port": 80,
            "destinations": [
                "node-js-docker"
            ]
        }
    ]
}