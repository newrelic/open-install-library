{
  "global_tags": {
    "owning_team": "virtuoso",
    "Environment": "development",
    "Department": "product",
    "Product": "virtuoso"
  },
  "resources": [
    {
      "id": "javaHost",
      "provider": "aws",
      "type": "ec2",
      "size": "t3.small",
      "ami_name": "amzn2-ami-hvm-2.0.????????.?-x86_64-gp2",
      "user_name": "ec2-user"
    }
  ],
  "services": [
    {
      "id": "javaApp",
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/python/install/rhel/roles/",
      "port": 80,
      "destinations": ["javaHost"]
    },
    {
      "id": "sysdtomcat",
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/tomcat/systemd/shScript/catalina/install/linux/roles",
      "port": 6001,
      "destinations": ["javaHost"]
    },
    {
      "id": "jmx-tomcat",
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/jmx-tomcat/install/rhel/roles",
      "port": 6002,
      "destinations": ["javaHost"]
    }
  ]
}
