{
    "global_tags": {
        "owning_team": "virtuoso",
        "Environment": "development",
        "Department": "product",
        "Product": "virtuoso"
    },

    "resources": [{
        "id": "ubuntunginxnetcore1",
        "provider": "aws",
        "type": "ec2",
