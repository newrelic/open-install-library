{
  "global_tags": {
    "owning_team": "OpenSource",
    "Environment": "development",
    "Department": "Product",
    "Product": "Virtuoso"
  },
  "resources": [{
      "id": "host1",
      "display_name": "AwsLinux2ASPNETCore",
