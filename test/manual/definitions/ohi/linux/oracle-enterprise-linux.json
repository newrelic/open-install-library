{
  "global_tags": {
    "owning_team": "virtuoso",
    "Environment": "development",
    "Department": "product",
    "Product": "virtuoso"
  },
  "resources": [
    {
      "id": "oracleLinux",
      "provider": "aws",
      "type": "ec2",
      "size": "t3..medium",
      "ami_name": "cg-12e*",
      "user_name": "ec2-user"
    }
  ],

  "services": [
    {
      "id": "goOracle",
      "destinations": ["oracleLinux"],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/oracle/install/linux/roles",
      "port": 9999
    }
  ]
}
