  "services": [
    {
      "id": "mongodb1",
      "destinations": [
        "host1"
      ],
      "source_repository": "https://github.com/newrelic/open-install-library.git",
      "deploy_script_path": "test/deploy/linux/mongodb/install/debian/roles",
      "port": 9999,
      "params": {
        "create_newrelic_user": true
      }
    }
  ]
}
