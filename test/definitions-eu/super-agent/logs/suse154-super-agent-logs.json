{
    "global_tags": {
        "owning_team": "virtuoso",
        "Environment": "development",
        "Department": "product",
        "Product": "virtuoso"
    },
    "resources": [
        {
            "id": "host1",
            "provider": "aws",
            "type": "ec2",
            "size": "t3.nano",
            "ami_name": "suse-sles-15-sp4-v????????-hvm-*"
        }
    ],
    "instrumentations": {
        "resources": [
            {
                "id": "nr_super_agent_logs",
                "resource_ids": ["host1"],
                "provider": "newrelic",
                "source_repository": "https://github.com/newrelic/open-install-library",
                "deploy_script_path": "test/deploy/linux/newrelic-cli/install/roles",
                "params": {
                    "newrelic_cli_option": "-n logs-integration-super-agent",
                    "validate_output": "Logs Integration\\s+\\(installed\\)",
                    "local_recipes": true
                }
            }
        ]
    }
}